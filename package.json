    "passport-google-oauth20": "^1.0.0",
    "passport-trello": "^0.1.5",
    "request": "^2.69.0",
    "restify": "^4.0.3",
    "restify-redirect": "^1.0.0",
    "socket.io": "^1.4.5",
    "timed-cache": "^1.0.5"
