{
  "name": "air-traffic-control",
  "version": "0.1.0",
  "description": "Internal project status tracking",
  "main": "index.js",
  "scripts": {
    "lint": "jshint --config .jshintrc --exclude ./node_modules,./web/static .",
    "build-style": "node-sass web/src/style/main.scss -o web/static --include-path `node sassPaths.js`",
    "build-html": "browserify -t [ babelify --presets [ react es2015 ] ] web/src/index.js -o web/static/bundle.js",
    "build": "npm run build-style && npm run build-html",
    "start": "node server/main.js"
  },
  "author": "",
  "license": "CC0-1.0",
  "dependencies": {
    "client-sessions": "^0.7.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "request": "^2.69.0",
    "restify": "^4.0.3",
    "restify-redirect": "^1.0.0",
    "socket.io": "^1.4.5"
  },
  "devDependencies": {
    "18f-contrib-web-design-standards": "^1.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-request": "^0.3.3",
    "browserify": "^12.0.0",
    "es6-promise": "^3.0.2",
    "flux": "^2.1.1",
    "jshint": "^2.9.1",
    "local-storage": "^1.4.2",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-select": "^1.0.0-beta8",
    "ucfirst": "^1.0.0"
  },
  "engines": {
    "node": "4.2.5",
    "npm": "3.5.3"
  }
}
